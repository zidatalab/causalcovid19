	keywords = {*Health Communication, *Health Knowledge, Attitudes, Practice, *Pandemics, *public health, Betacoronavirus, Coronavirus, Coronavirus Infections/*epidemiology, Data Mining, Disease Outbreaks, Global Health, Humans, Pneumonia, Viral/*epidemiology},
