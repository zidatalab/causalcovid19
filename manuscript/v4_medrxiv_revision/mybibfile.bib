	keywords = {* basic reproduction number, * COVID-19, * mathematical model, * media reporting, * sensitivity analysis, *Betacoronavirus, *Communication, *Mass Media, Basic Reproduction Number, China/epidemiology, Communicable Disease Control/methods, Computer Simulation, Coronavirus Infections/epidemiology/*prevention \& control, Humans, Models, Theoretical, Pandemics/*prevention \& control, Pneumonia, Viral/epidemiology/*prevention \& control, Public Health},
	pages = {2693--2707},
	file = {Zhou et al Effects of Media Reporting on Mitigating Spread of COVID-19 in the Early Phase of the Outbreak.pdf:C\:\\Users\\tmussgnug\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\4zquj304.default\\zotero\\storage\\7TBJ6CW9\\Zhou et al Effects of Media Reporting on Mitigating Spread of COVID-19 in the Early Phase of the Outbreak.pdf:application/pdf}
